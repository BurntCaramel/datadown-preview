{
  "name": "datadown-preview",
  "version": "0.1.0",
  "description": "Describe datadown-preview here",
  "scripts": {
    "build": "nwb build-web-app && cp src/_redirects dist/_redirects",
    "clean": "nwb clean-app",
    "dev": "nwb serve-web-app --reload",
    "test": "nwb test",
    "test:coverage": "nwb test --coverage",
    "test:watch": "nwb test --server"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      }
    }
  },
  "dependencies": {},
  "devDependencies": {
    "elm": "^0.18.0",
    "elm-webpack-loader": "^4.4.0",
    "nwb": "0.21.x",
    "tailwind": "^1.0.1",
    "tailwindcss": "^0.4.0"
  },
  "author": "",
  "license": "MIT",
  "repository": ""
}
