{
  "name": "datadown-preview",
  "version": "0.1.0",
  "description": "Describe datadown-preview here",
  "scripts": {
    "build": "nwb build-web-app && cp src/_redirects dist/_redirects",
    "clean": "nwb clean-app",
    "dev": "nwb serve-web-app --reload",
    "test:elm": "elm-test",
    "test": "npm run test:elm && nwb test",
    "test:coverage": "nwb test --coverage",
    "test:watch": "nwb test --server"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      }
    }
  },
  "dependencies": {},
  "devDependencies": {
    "elm": "^0.19.0-bugfix2",
    "elm-format": "^0.8.1",
    "elm-test": "^0.18.12",
    "elm-webpack-loader": "^5.0.0",
    "nwb": "^0.23.0",
    "tailwindcss": "^0.7.2"
  },
  "author": "",
  "license": "MIT",
  "repository": ""
}
